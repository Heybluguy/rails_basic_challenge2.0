<%= form_for [@student, @address] do |f| %>
  <%= f.label :description %>
  <%= f.text_field :description %><br>
  <%= f.label :street %>
  <%= f.text_field :street %><br>
  <%= f.label :city %>
  <%= f.text_field :city %><br>
  <%= f.label :state %>
  <%= f.text_field :state %><br>
  <%= f.label :zipcode %>
  <%= f.text_field :zipcode %><br>

  <%= f.submit %>
<% end %>